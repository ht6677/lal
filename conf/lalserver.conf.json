{
  "rtmp": {
    "enable": true,
    "addr": ":19350",
    "gop_num": 2
  },
  "httpflv": {
    "enable": true,
    "sub_listen_addr": ":8080",
    "enable_https": false,
    "https_addr": ":4433",
    "https_cert_file": "./conf/cert.pem",
    "https_key_file": "./conf/key.pem",
    "gop_num": 2
  },
  "hls": {
    "enable": true,
    "sub_listen_addr": ":8081",
    "out_path": "/tmp/lal/hls/",
    "fragment_duration_ms": 3000,
    "fragment_num": 6
  },
  "httpts": {
    "enable": true,
    "sub_listen_addr": ":8082"
  },
  "rtsp": {
    "enable": true,
    "addr": ":5544"
  },
  "relay_push": {
    "enable": false,
    "addr_list":[
    ]
  },
  "relay_pull": {
    "enable": false,
    "addr": ""
  },
  "pprof": {
    "enable": true,
    "addr": ":10001"
  },
  "log": {
    "level": 1,
    "filename": "./logs/lalserver.log",
    "is_to_stdout": true,
    "is_rotate_daily": true,
    "short_file_flag": true,
    "assert_behavior": 1
  }
}
